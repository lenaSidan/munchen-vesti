    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.0.0",
    "rehype-external-links": "^3.0.0",
    "rehype-stringify": "^10.0.1",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
